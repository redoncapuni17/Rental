<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

         <record id="seq_invoice" model="ir.sequence">
            <field name="name">Invoice Sequence</field>
            <field name="code">invoice.cp</field>
            <field name="prefix">INV/</field>
            <field name="padding">7</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
       </record>

         <record id="seq_payment" model="ir.sequence">
            <field name="name">Payment Sequence</field>
            <field name="code">payment.cp</field>
            <field name="prefix">PAYMENT/</field>
            <field name="padding">7</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
       </record>

    </data>
</odoo>