<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="booking_sequence" model="ir.sequence">
            <field name="name">Rental Booking Sequence</field>
            <field name="code">rental.booking.seq</field>
            <field name="prefix">BOOKING/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range">True</field>
        </record>


    </data>
</odoo>
